
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>

  <%= devise_error_messages! %>
<div class="large-8 large-centered columns"> -->
  <div class="login-box">
  <div class="row">
  <div class="large-12 columns">
    <form>
       <div class="row">
         <div class="large-12 columns">
          <%= f.label :email %>
          <%= f.email_field :email, autofocus: true, :placeholder => "Enter Email" %>
          </div>
       </div>
      <div class="row">
         <div class="large-12 columns">
          <%= f.label :password %>
          <% if @minimum_password_length %>
          <em><font color="000000">(<%= @minimum_password_length %> characters minimum)</font></em>
          <% end %>
          <%= f.password_field :password, autocomplete: "off", :placeholder => "Enter Password" %>
         </div>
      </div>
      <div class="row">
         <div class="large-12 columns">
        <%= f.label :password_confirmation %>
        <%= f.password_field :password_confirmation, autocomplete: "off", :placeholder => "Re-enter Password" %>
        </div>
      </div>


      <div class="row">
         <div class="large-12 columns">
          <%= f.label :Profile_image_URL %>
          <%= f.text_field :profile_img %>
        </div>
      </div>

  <div class="row">
    <div class="large-4 columns">

          <%= f.label :Name %>
          <%= f.text_field :name, :placeholder => "Enter Name" %>

    </div>
    <div class="large-4 columns">
      <div class="row collapse">
        <label>Weight</label>
        <div class="small-9 columns">
         <!--  HAD TO USE RAW HTML BELOW ONCE -->
          <input type="text" name="user[weight]" id="user_name" placeholder="Enter Weight" />
          <!-- END -->
        </div>
        <div class="small-3 columns">
          <span class="postfix">lbs</span>
        </div>
      </div>
    </div>
  <div class="row">
    <div class="large-4 columns">
      <label>Age
        <br>
          <%= f.select( :age, (18..99).to_a) %>
      </label>
    </div>
  </div>
    <div class="large-4 columns">
          <%= f.label :Height %>
          <%= f.text_field :height, :placeholder => "Enter Height" %>
    </div>
    <div class="large-4 columns">

        <%= f.label :Weight_goal %>
        <%= f.text_field :weight_goal, :placeholder => "Enter Weight Goal" %>

    </div>
    <div class="large-4 columns">
      <label>Gender
      <%= f.select :sex, [['Male','Male'],['Female','Female'], ['Other','Other']] %>
      </label>
    </div>
  </div>
  <div class='row'>
      <div class="large-4 columns">
          <label>Daily Protien Intake<br>(Leave blank if unknown)</label>
          <%= f.text_field :d_protein_r, :placeholder => "Enter Proteins" %>
    </div>
    <div class="large-4 columns">

        <label>Daily Fat Intake<br>(Leave blank if unknown)</label>
        <%= f.text_field :d_carb_r, :placeholder => "Enter Carbs" %>

    </div>
    <div class="large-4 columns">

        <label>Daily Carb Intake<br>(Leave blank if unknown)</label>
        <%= f.text_field :d_fat_r, :placeholder => "Enter Fats" %>

    </div>
  </div>

  <br><br>




      <div class="row">
        <div class="large-5 large-centered columns">
          <%= f.submit "Register", :class => "button success expand" %>
          <%= link_to "Login", new_user_session_path, :class => "button tiny expand" %>
          <%= link_to "Forgot Password", new_password_path(resource_name), :class => "button tiny alert expand" %>
          <%= link_to "Resend Confirmation", new_confirmation_path(resource_name), :class => "button tiny info expand" %>
        </div>
      </div>
    </form>
  </div>
</div>
</div>
</div>





<% end %>







